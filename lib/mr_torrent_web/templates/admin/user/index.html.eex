<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Admin</th>
    </tr>
  </thead>
  <tbody>
  <%= for user <- @users.list do %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= MrTorrentWeb.PaginatorHelper.render(@conn, @users, class: "pagination", name: "users") %>
